-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
ELFSIZE += \
hello.elf.size \

ELFCHECK += \
hello.elf.elfcheck \


# All Target
all: hello.elf secondary-outputs

# Tool invocations
hello.elf: $(OBJS) ../src/lscript.ld $(USER_OBJS)
	@echo Building target: $@
	@echo Invoking: PowerPC gcc linker
	powerpc-eabi-gcc -Wl,-T -Wl,../src/lscript.ld -L../../hello_bsp/ppc440_0/lib -mcpu=440 -o"hello.elf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo Finished building target: $@
	@echo ' '

hello.elf.size: hello.elf
	@echo Invoking: PowerPC Print Size
	powerpc-eabi-size hello.elf  |tee "hello.elf.size"
	@echo Finished building: $@
	@echo ' '

hello.elf.elfcheck: hello.elf
	@echo Invoking: Xilinx ELF Check
	elfcheck hello.elf -hw ../../ReFrESH_Bubble1_to_Bubble4_hw_platform/system.xml -pe ppc440_0  |tee "hello.elf.elfcheck"
	@echo Finished building: $@
	@echo ' '

# Other Targets
clean:
	-$(RM) $(OBJS)$(C_DEPS)$(EXECUTABLES)$(ELFSIZE)$(ELFCHECK)$(S_UPPER_DEPS) hello.elf
	-@echo ' '

secondary-outputs: $(ELFSIZE) $(ELFCHECK)

.PHONY: all clean dependents
.SECONDARY:

-include ../makefile.targets
